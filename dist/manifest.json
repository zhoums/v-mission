{
  "manifest_version": 2,
  "name": "v-mission.0",
  "description": "",
  "version": "1.0.0",
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_title": "willbePlug1.0",
    "default_icon": "images/icon.png"
    // "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "downloads",
    "storage",
    "unlimitedStorage",
    "background",
    "*://*.taobao.com/*",
    "http://molitest.willbe.net.cn/*",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "page": "background.html"
  },
  "content_scripts": [{
    "matches": ["https://market.m.taobao.com/*", "https://*.chowsangsang.com/*"],
    "js": ["js/jquery.min.js", "content.js"]
  }]
}
